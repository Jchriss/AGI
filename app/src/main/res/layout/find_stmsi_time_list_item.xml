<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:drawingCacheQuality="high"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/find_stmsi_time_list_item_stmsi"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:gravity="center" />

    <TextView
        android:id="@+id/find_stmsi_time_list_item_time"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/find_stmsi_time_list_item_stmsi"
        android:layout_centerVertical="true"
        android:gravity="center" />

    <TextView
        android:id="@+id/find_stmsi_time_list_item_pci"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/find_stmsi_time_list_item_time"
        android:layout_centerVertical="true"
        android:gravity="center" />

    <TextView
        android:id="@+id/find_stmsi_time_list_item_board"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/find_stmsi_time_list_item_pci"
        android:layout_centerVertical="true"
        android:gravity="center" />

</RelativeLayout>

